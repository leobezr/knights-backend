   - level / exp
      - (level) => Math.round(50 * Math.pow(level, 3) / 3) - Math.round(100 * Math.pow(level, 2)) + Math.round(850 * Math.pow(level, 1) / 3) - 200
      - status 7 points per level
         - (level, totalAttr) => Math.round(Math.abs(totalAttr - (level *  7)))
